{
  "compilerOptions": {
    "rootDir": "./src/",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "moduleResolution": "node",
    // "ts-node": {},

},

"include": [ "src/**/*", "server/**/*.js" ],
  "exclude": ["node_modules", "dist"],
}