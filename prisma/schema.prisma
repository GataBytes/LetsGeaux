
generator client {
  provider = "prisma-client-js"
  // previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id Int @id @default(autoincrement())
  username String @db.VarChar(50)
  email String @db.VarChar(50)
  isVerified Boolean
  phoneNum String @db.VarChar(10)
  isNotified Boolean
}